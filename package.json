{
  "name": "03-api-solid",
  "version": "0.0.1",
  "description": "API nodejs - Rockeseat - Ignite - 2023 - Projeto 03",
  "main": "server.ts",
  "author": "Pedro Machado de Paula",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "cd prisma/vitest-environment-prisma && yarn link",
    "test:install-prisma-environment": "yarn link vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "lint": "eslint --ext .ts src",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --api 9527",
    "p:studio": "npx prisma studio"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.0",
    "@prisma/client": "4.16.2",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.19.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.4.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-module-resolver": "1.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "prisma": "4.16.2",
    "prisma-docs-generator": "0.8.0",
    "supertest": "6.3.3",
    "tsup": "7.1.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
}
